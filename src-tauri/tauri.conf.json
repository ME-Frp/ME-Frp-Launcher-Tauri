{
  "$schema": "https://schema.tauri.app/config/2",
  "productName": "ME Frp Launcher",
  "version": "1.0.0",
  "identifier": "com.mefrp.launcher",
  "build": {
    "beforeDevCommand": "bun run vdev",
    "devUrl": "http://localhost:1420",
    "beforeBuildCommand": "bun run vbuild",
    "frontendDist": "../dist"
  },
  "app": {
    "windows": [
      {
        "title": "ME Frp Launcher - 幻缘映射启动器",
        "width": 1100,
        "height": 720,
        "minWidth": 430,
        "minHeight": 600,
        "decorations": false,
        "transparent": true
      }
    ],
    "security": {
      "csp": null
    }
  },
  "bundle": {
    "active": true,
    "targets": "all",
    "icon": [
      "icons/32x32.png",
      "icons/64x64.png",
      "icons/128x128.png",
      "icons/128x128@2x.png",
      "icons/icon.icns",
      "icons/icon.ico"
    ]
  }
}